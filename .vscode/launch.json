{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "cppdbg",
      "request": "launch",
      "name": "Launch Tracker",
      "program": "${workspaceFolder}/tracker.exe", // Path to your existing executable
      "args": ["test2.song"], // Replace <filename> with the actual filename or pass it as an argument
      "stopAtEntry": true,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "miDebuggerPath": "c:/msys64/mingw64/bin/gdb.exe", // Adjust this path if necessary
      "logging": {
        "engineLogging": true
      }
    }
  ]
}